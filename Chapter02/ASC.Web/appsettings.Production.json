{
  //  This is our "production" setting, which is different than the appsettings.json or the
  //  appsettings.development.json files.  This is an easy way to see which environment we are
  //  working and in the real world, this kind of application title is used to ensure you can
  //  easily know which environment you are working inside.
  "AppSettings": {
    "ApplicationTitle": "Automobile Service Center Production Application"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
