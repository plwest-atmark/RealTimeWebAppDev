{
  //  This is our custom setting for the title if there is no environment specific appsettings.json file
  //  In this example, we are using this as the default, but will be creating a development and production
  //  appsettings.{environment}.json file to override this setting when we are in specific environments.
  "AppSettings": {
    "ApplicationTitle": "Automobile Service Center Application"
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=.\\ASC.Web.db"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
